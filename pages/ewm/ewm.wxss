/* pages/ewm/ewm.wxss */
.ts{
  width:100%;
  height:160rpx;
  background: #64c7f1;
  display: flex;
  justify-content: center;
  align-items: center;
}
.inner_ts{
  width:700rpx;
  font-size:25rpx;
  color:#fff;
  text-align: center;
  font-weight: bold;
}
.body{
  width:100%;
  border-top:2px solid #fff;
  padding-top:30rpx;
  height:600rpx;
}
.body_title{
  padding:10rpx 10rpx;
  font-size:25rpx;
  font-weight: bold;
  color:#666;
  text-align: center;
}
.body_title2{
  padding:10rpx 10rpx;
  font-size:25rpx;
  color:#e55710;
  font-weight: bold;
  text-align: center;
  padding-bottom: 30rpx;
}
.ewm{
  width:80%;
  margin:0 auto;
  height:500rpx;
  border:1px solid #ccc;
}
.ewm image{
  width:100%;
  height:100%;
}
.success{
  width:80%;
  margin:0 auto;
  margin-top:50px;
  height:180rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.success-title{
  text-align: center;
  font-size:40rpx;
  font-weight: bold;
  color:#e55710;
}
.success-menu{
  margin-top:30rpx;
  display: flex;
  justify-content: space-evenly;
  align-items: center;
}
.success-menu view{
  font-size:30rpx;
  text-align: center;
  padding:10rpx 20rpx;
  border:1px solid #ccc;
}