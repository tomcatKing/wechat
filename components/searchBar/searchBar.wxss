/* components/searchBar/searchbar.wxss */
.searchbar{
  background: #64c7f1;
  padding:20rpx;
}

.search-navigator{
  width:100%;
  height:60rpx;
  background: #fff;
  border-radius: 10rpx;
  background-image: url("data:image/png;base64,");
  background-position: center center;
  background-repeat:no-repeat;
  background-size:6%;
}

.search-input-group{
  width:100%;
  height:60rpx;
  background: #fff;
  border-radius: 10rpx;
  padding:10rpx 20rpx;
  box-sizing: border-box;
}

.search-input{
  min-height:40rpx;
  height:30rpx;
  line-height: 30rpx;
  font-size:27rpx;
}
